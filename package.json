{
  "name": "shipengine",
  "version": "1.0.0-alpha.3",
  "description": "ShipEngine SDK for node / browser",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "cjs",
    "esm"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "gen": "sh generate-internal-client.sh",
    "tsc:check": "tsc --noEmit",
    "test": "npm run test:node && npm run test:browser",
    "test:node:watch": "chokidar **/test/** **/src/**/*.ts -c 'npm run test:node'",
    "test:browser:watch": "chokidar **/test/** karma.conf.js -c 'npm run test:browser -- --single-run'",
    "test:node": "npm run build:cjs && mocha",
    "test:browser": "npm run build && karma start --single-run",
    "lint": "npm run tsc:check && eslint .",
    "lint:examples": "eslint docs/examples/**/*.ts",
    "update:sim": "git submodule update --recursive --remote",
    "fix": "npm run lint -- --fix",
    "devdoc": "chokidar typedoc.js src/** -c 'npm run compile:api'",
    "fix:examples": "npm run lint:examples -- --fix",
    "compile:lit": "ts-node-script docs/md-to-lit.ts",
    "compile:api": "typedoc",
    "compile:api:watch": "run-p devdoc serve",
    "compile:tutorials": "npm run compile:lit && lit --weave docs/dist/addresses.lit -odir docs/tutorials",
    "compile:examples": "npm run compile:lit && lit --tangle docs/dist/addresses.lit -odir docs/examples && npm run fix:examples",
    "compile": "npm run compile:api && npm run compile:tutorials && npm run compile:examples",
    "serve": "live-server ./docs",
    "bump": "bump prerelease --preid alpha --push && bash scripts/create-gh-release.sh"
  },
  "engines": {
    "node": "12.x"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@jsdevtools/host-environment": "^2.1.2",
    "@jsdevtools/karma-config": "^3.1.7",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@openapitools/openapi-generator-cli": "^2.0.3",
    "@tsconfig/node12": "^1.0.7",
    "@types/karma": "^5.0.1",
    "@types/lodash.curry": "^4.1.6",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.19.3",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "fkill": "^7.0.1",
    "karma": "^5.2.3",
    "karma-child-process": "^1.0.4",
    "karma-cli": "^2.0.0",
    "live-server": "^1.2.1",
    "mocha": "^8.2.1",
    "mocha-each": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "shipengine-json-schema": "^1.4.0",
    "shipengine-openapi": "^1.4.0",
    "sinon": "^9.2.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typescript": "^4.0.5",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "lodash.curry": "^4.1.1",
    "retry-axios": "^2.3.0"
  }
}
